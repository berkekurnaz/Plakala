@model Yorum
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    <link rel="stylesheet" href="~/dosyalartemp/src/css/thirdpage.css">
    <title>Plakala</title>
</head>
<body>

    <main class="Main">
        <div class="timeline">
            <div class="box">
                <div class="icon">
                    <i class="fas fa-car fa-2x"> </i>
                </div>
                <div class="content">
                    <div class="title">
                        <p>Plaka</p>
                    </div>
                    <div class="text">
                        <p>@Model.Plaka</p>
                    </div>
                </div>
            </div>
            <div class="box">
                <div class="icon">
                    <i class="fas fa-car fa-2x"> </i>
                </div>
                <div class="content">
                    <div class="title">
                        <p>Eklenme Tarihi</p>
                    </div>
                    <div class="text">
                        <p>@Model.EklenmeTarihi</p>
                    </div>
                </div>
            </div>
            <div class="box">
                <div class="icon">
                    <i class="fas fa-car fa-2x"> </i>
                </div>
                <div class="content">
                    <div class="title">
                        <p>Yorum</p>
                    </div>
                    <div class="text">
                        <p>@Model.PlakaYorum</p>
                    </div>
                </div>
            </div>
        </div>
    </main>

</body>
</html>